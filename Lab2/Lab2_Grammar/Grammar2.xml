<?xml version="1.0" encoding="UTF-8"?>
<!--4.11-->
<grammar name="G2">
  <terminalsymbols>
    <term name="a" spell="a" />
    <term name="b" spell="b" />
    <term name="IF" spell="i" />
    <term name="THEN" spell="t" />
    <term name="ELSE" spell="e" />
    <term name="EPS" spell="eps" />
  </terminalsymbols>
  <nonterminalsymbols>
    <nonterm name="S" />
    <nonterm name="E" />
  </nonterminalsymbols>
  <productions>
    <production>
      <lhs name="S" />
      <rhs>
        <symbol type="term" name="IF" />
        <symbol type="nonterm" name="E" />
        <symbol type="term" name="THEN" />
        <symbol type="nonterm" name="S" />
      </rhs>
    </production>
    <production>
      <lhs name="S" />
      <rhs>
        <symbol type="term" name="IF" />
        <symbol type="nonterm" name="E" />
        <symbol type="term" name="THEN" />
        <symbol type="nonterm" name="S" />
        <symbol type="term" name="ELSE" />
        <symbol type="nonterm" name="S" />
      </rhs>
    </production>
    <production>
      <lhs name="S" />
      <rhs>
        <symbol type="term" name="a" />
      </rhs>
    </production>
    <production>
      <lhs name="E" />
      <rhs>
        <symbol type="term" name="b" />
      </rhs>
    </production>
  </productions>
  <startsymbol name="S" />
</grammar>